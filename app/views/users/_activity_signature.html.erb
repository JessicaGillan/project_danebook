<div class="row">
  <div class="col-xs-12">
    <%= link_to  image_tag(user.profile.profile_photo_url(:thumb), alt: "Profile Image", class: "pull-left custom-poster thumbnail"),
                 user_path(user) %>

    <%= link_to user.name, user_path(user) %>
    <p>Posted <time datetime="<%= object.created_at %>"><%= time_ago_in_words(object.created_at) + " ago" %></time></p>
  </div>
</div>
