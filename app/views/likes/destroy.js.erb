(function() {
  var $likeLink = $("[data-type='<%= @likable.type %>'][data-id='<%= @likable.id %>'] .like-link").eq(0);
  $likeLink.html("<%= j(render partial: 'shared/like_link', locals: { likable: @likable }) %>")

  var $likes = $("[data-type='<%= @likable.type %>'][data-id='<%= @likable.id %>'] .likes").eq(0);
  $likes.html('<%= list_of_likes(@likable) %>')
})();

